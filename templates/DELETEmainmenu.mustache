<div id="{{M_MENUBUTTONS_CONTAINER}}" class="{{M_MENUBUTTONS_CONTAINER}}">
    <ul class="list-unstyled p-0">
        <li class="nav-item" id="{{M_HOME}}">
            <a href="javascript:void(0)" class="nav-link h6 w-100" data-mode="home">
                <div class="nav-text-group">
                    <span class="nav-label">{{#str}} home, mod_readaloud {{/str}}</span>
                </div>
            </a>
        </li>
        {{#stepsenabled.step_listen}}
        <li class="nav-item mode-chooser {{^stepsopen.step_listen}}no-click{{/stepsopen.step_listen}}" data-step="{{steps.step_listen}}">
            <a href="javascript:void(0)" class="nav-link h6 w-100" data-mode="preview">
                <div class="nav-text-group">
                    <span class="nav-label">{{#str}} mode_listen, mod_readaloud {{/str}}</span>
                    {{#stepsopen.step_listen}}
                        {{#stepscomplete.step_listen}}
                            <span class="nav-completion-icon ml-auto">
                                <i class="fa-solid fa-circle-check text-success" title="Complete"></i>
                            </span>
                        {{/stepscomplete.step_listen}}
                        {{^stepscomplete.step_listen}}
                            <span class="nav-completion-icon ml-auto">
                                <i class="fa-regular fa-circle text-warning" title="In progress"></i>
                            </span>
                        {{/stepscomplete.step_listen}}
                    {{/stepsopen.step_listen}}
                    {{^stepsopen.step_listen}}
                        <span class="nav-completion-icon ml-auto">
                            <i class="fa-solid fa-lock text-secondary" title="Locked"></i>
                        </span>
                    {{/stepsopen.step_listen}}
                </div>
            </a>
        </li>
        {{/stepsenabled.step_listen}}

        {{#stepsenabled.step_practice}}
        <li class="nav-item mode-chooser {{^stepsopen.step_practice}}no-click{{/stepsopen.step_practice}} {{^hasaudiobreaks}}no-click{{/hasaudiobreaks}}" data-step="{{steps.step_practice}}">
            <a href="javascript:void(0)" class="nav-link h6 w-100" data-mode="landr">
            <div class="nav-text-group">
                <span class="nav-label">{{#str}} mode_practice, mod_readaloud {{/str}}</span>
                {{#stepsopen.step_practice}}
                    {{#stepscomplete.step_practice}}
                        <span class="nav-completion-icon ml-auto">
                            <i class="fa-solid fa-circle-check text-success" title="Complete"></i>
                        </span>
                    {{/stepscomplete.step_practice}}
                    {{^stepscomplete.step_practice}}
                        <span class="nav-completion-icon ml-auto">
                            <i class="fa-regular fa-circle text-warning" title="In progress"></i>
                        </span>
                    {{/stepscomplete.step_practice}}
                {{/stepsopen.step_practice}}
                {{^stepsopen.step_practice}}
                    <span class="nav-completion-icon ml-auto">
                        <i class="fa-solid fa-lock text-secondary" title="Locked"></i>
                    </span>
                {{/stepsopen.step_practice}}
            </div>
            </a>
            {{^hasaudiobreaks}}
            <span class="mod_readaloud_menubutton_warning">{{#str}} modelaudiowarning, mod_readaloud {{/str}}</span>
            {{/hasaudiobreaks}}
        </li>
        {{/stepsenabled.step_practice}}

        {{#stepsenabled.step_shadow}}
        <li class="nav-item mode-chooser {{^stepsopen.step_shadow}}no-click{{/stepsopen.step_shadow}} {{^canshadowattempt}}no-click{{/canshadowattempt}}" data-step="{{steps.step_shadow}}">
            <a href="javascript:void(0)" class="nav-link h6 w-100" data-mode="shadow">
                <div class="nav-text-group">
                    <span class="nav-label">{{#str}} mode_shadow, mod_readaloud {{/str}}</span>
                    {{#stepsopen.step_shadow}}
                        {{#stepscomplete.step_shadow}}
                            <span class="nav-completion-icon ml-auto">
                                <i class="fa-solid fa-circle-check text-success" title="Complete"></i>
                            </span>
                        {{/stepscomplete.step_shadow}}
                        {{^stepscomplete.step_shadow}}
                            <span class="nav-completion-icon ml-auto">
                                <i class="fa-regular fa-circle text-warning" title="In progress"></i>
                            </span>
                        {{/stepscomplete.step_shadow}}
                    {{/stepsopen.step_shadow}}
                    {{^stepsopen.step_shadow}}
                        <span class="nav-completion-icon ml-auto">
                            <i class="fa-solid fa-lock text-secondary" title="Locked"></i>
                        </span>
                    {{/stepsopen.step_shadow}}
                </div>
            </a>
            <!--{{^canshadowattempt}}
            <span class="mod_readaloud_menubutton_warning">{{#str}} exceededallattempts, mod_readaloud {{/str}}</span>
            {{/canshadowattempt}}-->
        </li>
        {{/stepsenabled.step_shadow}}

        {{#stepsenabled.step_read}}
        <li class="nav-item mode-chooser {{^stepsopen.step_read}}no-click{{/stepsopen.step_read}} {{^canattempt}}no-click{{/canattempt}}" data-step="{{steps.step_read}}">
            <a href="javascript:void(0)" class="nav-link h6 w-100" data-mode="startnoshadow">
                <div class="nav-text-group">
                    <span class="nav-label">{{#str}} mode_read, mod_readaloud {{/str}}</span>
                    {{#stepsopen.step_read}}
                        {{#stepscomplete.step_read}}
                            <span class="nav-completion-icon ml-auto">
                                <i class="fa-solid fa-circle-check text-success" title="Complete"></i>
                            </span>
                        {{/stepscomplete.step_read}}
                        {{^stepscomplete.step_read}}
                            <span class="nav-completion-icon ml-auto">
                                <i class="fa-regular fa-circle text-warning" title="In progress"></i>
                            </span>
                        {{/stepscomplete.step_read}}
                    {{/stepsopen.step_read}}
                    {{^stepsopen.step_read}}
                        <span class="nav-completion-icon ml-auto">
                            <i class="fa-solid fa-lock text-secondary" title="Locked"></i>
                        </span>
                    {{/stepsopen.step_read}}
                </div>
            </a>
            {{^canattempt}}
            <span class="mod_readaloud_menubutton_warning">{{#str}} exceededallattempts, mod_readaloud {{/str}}</span>
            {{/canattempt}}
        </li>
        {{/stepsenabled.step_read}}

        {{#stepsenabled.step_quiz}}
        <li class="nav-item mode-chooser {{^stepsopen.step_quiz}}no-click{{/stepsopen.step_quiz}} {{#noquiz}}no-click{{/noquiz}}" data-step="{{steps.step_quiz}}">
            <a href="javascript:void(0)" class="nav-link h6 w-100" data-mode="quiz">
                <div class="nav-text-group">
                    <span class="nav-label">{{#str}} mode_quiz, mod_readaloud {{/str}}</span>
                    {{#stepsopen.step_quiz}}
                        {{#stepscomplete.step_quiz}}
                            <span class="nav-completion-icon ml-auto">
                                <i class="fa-solid fa-circle-check text-success" title="Complete"></i>
                            </span>
                        {{/stepscomplete.step_quiz}}
                        {{^stepscomplete.step_quiz}}
                            <span class="nav-completion-icon ml-auto">
                                <i class="fa-regular fa-circle text-warning" title="In progress"></i>
                            </span>
                        {{/stepscomplete.step_quiz}}
                    {{/stepsopen.step_quiz}}
                    {{^stepsopen.step_quiz}}
                        <span class="nav-completion-icon ml-auto">
                            <i class="fa-solid fa-lock text-secondary" title="Locked"></i>
                        </span>
                    {{/stepsopen.step_quiz}}
                </div>
            </a>
            <!--{{#stepscomplete.step_quiz}}
            <span class="mod_readaloud_menubutton_warning">{{#str}} quizcompletedwarning, mod_readaloud {{/str}}</span>
            {{/stepscomplete.step_quiz}}-->
        </li>
        {{/stepsenabled.step_quiz}}

        {{#stepsenabled.step_report}}
        <li class="nav-item mode-chooser {{^stepsopen.step_report}}no-click{{/stepsopen.step_report}}" id="{{M_STARTREPORT}}" data-step="{{steps.step_report}}">
            <a href="javascript:void(0)" class="nav-link h6 w-100" data-mode="fullreport">
                <div class="nav-text-group">
                    <span class="nav-label">{{#str}} mode_report, mod_readaloud {{/str}}</span>
                    {{#stepsopen.step_report}}
                        {{#stepscomplete.step_read}}
                            <span class="nav-completion-icon ml-auto">
                                <i class="fa-solid fa-circle-check text-success" title="Complete"></i>
                            </span>
                        {{/stepscomplete.step_read}}
                        {{^stepscomplete.step_read}}
                            <span class="nav-completion-icon ml-auto">
                                <i class="fa-regular fa-circle text-warning" title="In progress"></i>
                            </span>
                        {{/stepscomplete.step_read}}
                    {{/stepsopen.step_report}}
                    {{^stepsopen.step_report}}
                        <span class="nav-completion-icon ml-auto">
                            <i class="fa-solid fa-lock text-secondary" title="Locked"></i>
                        </span>
                    {{/stepsopen.step_report}}
                </div>
            </a>
        </li>
        {{/stepsenabled.step_report}}
    </ul>
</div>
